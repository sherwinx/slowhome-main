{
  "rewrites": [
    {
      "source": "/proposal/:path*",
      "destination": "https://miao-surfing-game.vercel.app/:path*"
    },
    {
      "source": "/proposal",
      "destination": "https://miao-surfing-game.vercel.app/index.html"
    }
  ],
  "headers": [
    {
      "source": "/proposal/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}